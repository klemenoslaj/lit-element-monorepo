{
  "name": "first-page",
  "main": "./dist/index.js",
  "source": "./src/index.ts",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "parcel build src/index.ts",
    "build:dev": "parcel build src/index.ts --no-minify --no-scope-hoist --no-source-maps",
    "clear:dist": "rm -rf dist",
    "clear:cache": "rm -rf .parcel-cache",
    "clear:deps": "rm -rf node_modules",
    "clear:prune": "rm -rf dist node_modules .parcel-cache",
    "format": "prettier './src/**/*.{ts,html,scss}' --write"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@parcel/babel-preset-env": "^2.0.0-alpha.3",
    "@parcel/transformer-sass": "^2.0.0-nightly.277",
    "parcel": "^2.0.0-nightly.275",
    "prettier": "^2.0.5",
    "sass": "^1.26.5"
  },
  "dependencies": {
    "@material/typography": "^6.0.0",
    "lit-element": "^2.3.1",
    "tslib": "^2.0.0"
  },
  "sass": {
    "includePaths": [
      "./node_modules"
    ]
  }
}
